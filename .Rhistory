library(dplyr)
dfw <- mtcars %>% select(mpg, wt, disp, drat, qsec)
library(tidyr)
dflong <- gather(dfw)
# using dflong and dfw, we can create a ggplot2 histogram matrix that matches
# our desired output. can later turn this into a function.
library(ggplot2)
ggplot(dflong, aes(x=value)) +
geom_histogram(fill="steelblue", color="white") +
facet_wrap(~key, scales="free") +
theme_bw()
